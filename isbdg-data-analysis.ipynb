   "source": [
    "import pandas as pd\n",
    "import arff\n",
    "\n",
    "# Load the ARFF file\n",
    "with open('./cosmic.arff', 'r') as f:\n",
    "    data = arff.load(f)\n",
    "\n",
    "# Convert to DataFrame\n",
    "train = pd.DataFrame(data['data'], columns=[attr[0] for attr in data['attributes']])\n",
    "train.head()\n",
    "\n",
    "train.to_csv('isbsg_data.csv', index=False)"
   ]
